{
  "name": "task-manager-infinitybase",
  "version": "1.0.0",
  "description": "An web app for managing tasks",
  "type": "module",
  "dependencies": {
    "bcrypt-ts": "^5.0.2",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "pg": "^8.11.5",
    "rsdi": "^3.0.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.14.0",
    "@types/uuid": "^9.0.8",
    "nodemon": "^3.1.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.11.2",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "start": "node ./build/index.js",
    "dev": "nodemon --exec tsx ./src/index.ts",
    "migration:create": "knex migrate:make",
    "migration:revert": "knex migrate:rollback",
    "migration:run": "knex migrate:latest",
    "seed:create": "knex seed:make",
    "seed:run": "knex seed:run",
    "postgres:start": "docker run -d -p $(grep DB_PORT .env | cut -d= -f 2):5432 --name $(grep DB_DATABASE .env | cut -d= -f 2) -e POSTGRES_HOST_AUTH_METHOD=trust postgres:12-alpine",
    "database:create": "docker exec $(grep DB_DATABASE .env | cut -d= -f 2) psql -U postgres postgres -c \"CREATE DATABASE $(grep DB_DATABASE .env | cut -d= -f 2)\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ViniciusEvans/task-manager-infinitybase"
  },
  "bugs": {
    "url": "https://github.com/ViniciusEvans/task-manager-infinitybase/issues"
  },
  "homepage": "https://github.com/ViniciusEvans/task-manager-infinitybase#readme"
}
